{
  "type": "project",
  "license": "proprietary",
  "require": {
    "php": "^7.4",
    "ext-iconv": "*",
    "ext-ldap": "*",
    "doctrine/annotations": "^1.0",
    "doctrine/doctrine-bundle": "^2.1",
    "doctrine/doctrine-migrations-bundle": "^3.0",
    "doctrine/orm": "^2.8",
    "endroid/qr-code-bundle": "^4.0",
    "evrinoma/contragent-bundle": "dev-master",
    "evrinoma/dashboard-bundle": "dev-master",
    "evrinoma/delta8-bundle": "dev-master",
    "evrinoma/exim-bundle": "dev-master",
    "evrinoma/grid-bundle": "dev-master",
    "evrinoma/livevideo-bundle": "dev-master",
    "evrinoma/menu-bundle": "dev-master",
    "evrinoma/project-bundle": "dev-master",
    "evrinoma/user-bundle": "dev-master",
    "friendsofsymfony/jsrouting-bundle": "^2.3",
    "friendsofsymfony/rest-bundle": "^2.5",
    "jms/serializer-bundle": "^3.1",
    "knplabs/knp-menu-bundle": "^3.0",
    "ltoscano/ponvif": "dev-master",
    "nelmio/api-doc-bundle": "dev-master",
    "sensio/framework-extra-bundle": "^5.2",
    "sensiolabs/security-checker": "^5.0",
    "symfony/asset": "^4.0",
    "symfony/console": "^4.0",
    "symfony/flex": "^1.1.8",
    "symfony/framework-bundle": "^4.0",
    "symfony/lts": "^4@dev",
    "symfony/orm-pack": "^1.0",
    "symfony/security-bundle": "^4.0",
    "symfony/swiftmailer-bundle": "^3.1",
    "symfony/templating": "^4.0",
    "symfony/twig-bundle": "^4.0",
    "symfony/yaml": "^4.0"
  },
  "require-dev": {
    "doctrine/doctrine-fixtures-bundle": "^3.2",
    "easycorp/easy-log-handler": "^1.0.2",
    "symfony/debug-bundle": "^3.3|^4.0",
    "symfony/dotenv": "^4.0",
    "symfony/monolog-bundle": "^3.0",
    "symfony/phpunit-bridge": "^5.2",
    "symfony/profiler-pack": "^1.0",
    "symfony/var-dumper": "^3.3|^4.0"
  },
  "config": {
    "github-oauth": {

    },
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": {
    "dashboard": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/DashBoardBundle.git"
    },
    "delta8": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/Delta8Bundle.git"
    },
    "dto": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/DtoBundle.git"
    },
    "grid": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/GridBundle.git"
    },
    "exim": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/EximBundle.git"
    },
    "shell": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/ShellBundle.git"
    },
    "contragent": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/ContrAgentBundle.git"
    },
    "livevideo": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/LiveVideoBundle.git"
    },
    "menu": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/MenuBundle.git"
    },
    "project": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/ProjectBundle.git"
    },
    "settings": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/SettingsBundle.git"
    },
    "utils": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/UtilsBundle.git"
    },
    "user": {
      "type": "vcs",
      "url": "https://github.com/evrinoma/FOSUserBundle.git"
    },
    "ponvif": {
      "type": "package",
      "package": {
        "name": "ltoscano/ponvif",
        "version": "dev-master",
        "dist": {
          "url": "https://github.com/ltoscano/ponvif/archive/master.zip",
          "type": "zip"
        },
        "autoload": {
          "classmap": ["lib/"]
        }
      }
    }
  },
  "replace": {
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php56": "*"
  },
  "scripts": {
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd"
    },
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ]
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "extra": {
    "symfony": {
      "id": "01C53TW0VP4PXDGXE78XYEYTGH",
      "allow-contrib": false
    }
  }
}
