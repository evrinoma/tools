<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Wellcome{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('build/style.css') }}">
    {% endblock %}
</head>
<body>
{% block body %}
    {% block menubar %}
        <div id="header">
            {% include 'menu/show.html.twig' with menu %}
        </div>
    {% endblock %}
    <div id="page">
        {% block page %}
        {% endblock %}
    </div>
{% endblock %}
{% block javascripts %}
    <script src="{{ asset("build/manifest.js") }}"></script>
    <script src="{{ asset("build/vendor.js") }}"></script>
    <script src="{{ asset("build/main.js") }}"></script>
{% endblock %}
</body>
</html>
